{ "version": 2, "rewrites": [{ "source": "/(.*)", "destination": "/api/index.js" }], 
"headers": [
    {
      "source": "/api/(.*)",
      "destination": "/api/index.js",
      "methods": ["GET", "POST", "PUT", "DELETE", "OPTIONS", "PATCH"],
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "https://review-app-front-iota.vercel.app"
        }
      ]
    }
  ]
}